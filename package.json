{
  "name": "markuplint",
  "version": "0.8.0",
  "description": "HTML linter for legacy/modern HTML, Web Components, SVG, MathML, AMP HTML and more.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "markuplint": "bin/index.js"
  },
  "author": "Yusuke Hirao",
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "git@github.com:YusukeHirao/markuplint.git"
  },
  "scripts": {
    "cli": "node ./bin/",
    "build": "gulp",
    "dev": "gulp watch",
    "test": "nyc ava && nyc report --reporter=html",
    "cover": "nyc ava && nyc report --reporter=text-lcov | coveralls",
    "scrape": "node ./rulesets/_scraper/"
  },
  "dependencies": {
    "cli-color": "^1.2.0",
    "deep-assign": "^2.0.0",
    "find-node-modules": "^1.0.4",
    "meow": "^4.0.0",
    "node-fetch": "^1.7.3",
    "os-locale": "^2.1.0",
    "parse5": "^3.0.3",
    "strip-ansi": "^4.0.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "@types/cli-color": "^0.3.29",
    "@types/meow": "^3.6.2",
    "@types/node-fetch": "^1.6.7",
    "@types/os-locale": "^2.1.0",
    "@types/strip-ansi": "^3.0.0",
    "@types/strip-json-comments": "^0.0.30",
    "ava": "^0.24.0",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^4.13.1",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.18.0",
    "gulp-typescript": "^3.2.3",
    "jsdom": "^11.5.1",
    "markuplint-plugin-textlint": "^0.2.0",
    "nyc": "^11.3.0",
    "run-sequence": "^2.2.0",
    "textlint": "^10.0.0",
    "textlint-rule-preset-japanese": "^2.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
