{
  "name": "markuplint",
  "version": "0.3.0",
  "description": "HTML linter for legacy/modern HTML, Web Components, SVG, MathML, AMP HTML and more.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "markuplint": "bin/index.js"
  },
  "author": "Yusuke Hirao",
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "git@github.com:YusukeHirao/markuplint.git"
  },
  "scripts": {
    "start": "node ./",
    "build": "gulp",
    "dev": "gulp watch",
    "test": "nyc ava && nyc report --reporter=html",
    "cover": "nyc ava && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "cli-color": "^1.2.0",
    "meow": "^4.0.0",
    "parse5": "^3.0.3"
  },
  "devDependencies": {
    "@types/cli-color": "^0.3.29",
    "@types/meow": "^3.6.2",
    "ava": "^0.24.0",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.18.0",
    "gulp-typescript": "^3.2.3",
    "nyc": "^11.3.0",
    "run-sequence": "^2.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
